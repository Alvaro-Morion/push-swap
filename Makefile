.c.o:
	@gcc -Wall -Werror -Wextra -c $< -o  ${<:.c=.o} 
all: ${NAME}

${NAME}: ${OBJS}
	@gcc -Wall -Werror -Wextra ${OBJS} -o ${NAME}
	@echo "Executable generated"
clean:
	@rm -rf ${OBJS}
	@echo ".o files removed"
fclean: clean
	@rm -rf ${NAME}
	@echo "binary file removed"
re: fclean all

norm: 
	@norminette ${SRCS} push_swap.h
test: ${NAME}
